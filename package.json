{
  "name": "pizza-cats",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@apollo/client": "^3.7.2",
    "@apollo/server": "^4.3.0",
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@mui/icons-material": "^5.11.0",
    "@mui/material": "^5.10.15",
    "@mui/styled-engine-sc": "^5.11.0",
    "@mui/styles": "^5.11.1",
    "@mui/system": "^5.11.1",
    "apollo": "^2.34.0",
    "apollo-client": "^2.6.10",
    "babel-jest": "^29.3.1",
    "dotenv": "^16.0.3",
    "gql": "^1.1.2",
    "graphql": "^15.8.0",
    "lodash": "^4.17.21",
    "mongodb": "^4.12.1",
    "node": "^16.0.0",
    "nodemon": "^2.0.20",
    "nvm": "^0.0.4",
    "os-browserify": "^0.3.0",
    "path-browserify": "^1.0.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^6.4.4",
    "react-scripts": "^5.0.1",
    "styled-components": "^5.3.6",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.3",
    "v4": "^0.0.1",
    "vercel": "^28.10.1"
  },
  "devDependencies": {
    "@babel/core": "^7.20.2",
    "@babel/plugin-syntax-jsx": "^7.18.6",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@types/jest": "^29.2.3",
    "@types/mongodb": "^4.0.7",
    "@types/node": "^18.11.17",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "@typescript-eslint/eslint-plugin": "^4.30.0",
    "@typescript-eslint/parser": "^4.30.0",
    "concurrently": "^5.3.0",
    "dotenv-webpack": "^8.0.1",
    "eslint": "^7.32.0",
    "eslint-plugin-testing-library": "^5.6.1",
    "husky": "^7.0.2",
    "jest": "^29.3.1",
    "lint-staged": "^11.0.0",
    "prettier": "^2.3.0",
    "process": "^0.11.10",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^4.11.1"
  },
  "optionalDependencies": {
    "fsevents": "*"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.17.0"
  },
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "install:all": "npm install --no-optional && cd packages/client && npm install --no-optional && cd ../server && npm install --no-optional",
    "start": "concurrently \"cd packages/client && npm run start\" \"cd packages/server && npm run start\"",
    "server:dev": "cd packages/server && npm start",
    "client:dev": "cd packages/client && npm start",
    "generate:types": "cd packages/server && npm run generate:types",
    "seed:database": "cd packages/server && npm run seed:database",
    "lint": "eslint \"**/*.{ts,js}\" --cache --fix",
    "format": "prettier --write \"**/*.{ts,js,json,graphql,gql,yaml,yml,md}\"",
    "format:check": "prettier --check \"**/*.{ts,js,json,graphql,gql,yaml,yml,md}\"",
    "prepare": "husky install",
    "build": "concurrently \"cd packages/client && npm run build\" \"cd packages/server && npm run build\"",
    "test": "cd packages/server && npm run test && cd ../client && npm run test --watchAll"
  },
  "lint-staged": {
    "*.{ts,tsx,js}": [
      "npm run generate:types",
      "npm run format",
      "npm run lint",
      "git add ."
    ],
    "*.js": "eslint --cache --fix",
    "*.{ts,js,json,graphql,gql,yaml,yml,md}": "prettier --write"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "description": "pizza ordering web app",
  "main": ".eslintrc.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mojonny/samCat.git"
  },
  "bugs": {
    "url": "https://github.com/mojonny/samCat/issues"
  },
  "homepage": "https://github.com/mojonny/samCat#readme",
  "author": "",
  "license": "ISC",
  "resolutions": {
    "@types/react": "17.0.2",
    "@types/react-dom": "17.0.2"
  },
  "preinstall": "npm install --package-lock-only --ignore-scripts && npx npm-force-resolutions"
}
